{
    "family": "llmopstdlive",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::963200076074:role/ecsTaskExecutionRole",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "1024",
    "memory": "8192",
    "containerDefinitions": [
      {
        "name": "llmopsrepolive",
        "image": "963200076074.dkr.ecr.us-east-1.amazonaws.com/llmopsrepolive",
        "cpu": 1024,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 8080,
            "hostPort": 8080,
            "protocol": "tcp",
            "name": "llmopsrepolive-8080-tcp",
            "appProtocol": "http"
          }
        ],
        "environment": [
          {
            "name": "ENV",
            "value": "production"
          }
        ],
        "secrets": [
          {
            "name": "apikeyliveclassTest",
            "valueFrom": "arn:aws:secretsmanager:us-east-1:963200076074:secret:apikeyliveclassTest-oHnRpE"
          }
  
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/llmopstdlive",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "ecs",
            "awslogs-create-group": "true"
          }
        }
      }
    ]
  }
  